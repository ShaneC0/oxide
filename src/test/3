init
    bool flag;
    int rand;
    flag = true;
    rand = (5 + 6) / 10 * (10 % 3);

    if(flag) then
        print("Flag is true and rand is: ", rand);
    else
        print("Flag is false!");
    endif;

    int counter = 0;
    while(counter < 10) do
        count = count + 1;
        print("The value of count is: ", count);
    endwhile;
halt